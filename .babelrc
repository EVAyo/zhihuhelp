{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "6.10"
        }
      }
    ],
    ["@babel/preset-react"],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "babel-plugin-root-import",
      {
        "rootPathSuffix": "./",
        "rootPathPrefix": "~"
      }
    ]
  ]
}
